{
  "name": "nodejs_crm",
  "version": "1.0.1",
  "description": "CRM",
  "main": "index.js",
  "scripts": {
    "start": "npm run client-install && npm run build-prod --prefix client && node index",
    "server": "nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start-tmp": "node index"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webenot/nodejs_crm.git"
  },
  "keywords": [
    "express",
    "angular",
    "nodejs"
  ],
  "author": "webenot",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webenot/nodejs_crm/issues"
  },
  "homepage": "https://github.com/webenot/nodejs_crm#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.12",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1"
  }
}
